# Build only NOR u-boot
UBOOT_MACHINES ?= "P1020RDB-PD"

# Wic support
# Support for SD boot has not been verified for P1020RDB. UBOOT_BINARY
# will be placed in the boot partition however so that the user may
# have a choice of manually updating u-boot

# With default switch settings, P1020RDB boots from primary NOR bank
# Following binary can be flashed to either NOR bank
UBOOT_BINARY ?= "u-boot-P1020RDB-PD.bin"

require conf/include/wic-ppc.inc
